{
  "name": "peacock-ui",
  "version": "0.0.0-development",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/peacock-ui/peacock-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kelvininc/peacock-ui.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "dev:stencil": "stencil build --watch",
    "dev": "run-p dev:stencil storybook",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0",
    "@stencil/react-output-target": "^0.2.1",
    "@stencil/angular-output-target": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@stencil/less": "^1.0.0",
    "@storybook/addon-actions": "^6.4.17",
    "@storybook/addon-essentials": "^6.4.17",
    "@storybook/addon-links": "^6.4.17",
    "@storybook/html": "^6.4.17",
    "@types/jest": "^27.0.3",
    "babel-loader": "^8.2.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^13.1.3",
    "semantic-release": "^19.0.2"
  },
  "license": "MIT"
}
